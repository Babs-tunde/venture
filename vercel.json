{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {"distDir": "staticfiles_build"}
    },
    {
      "src": "wholesale_app/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "50mb", "runtime": "python3.9"}
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "wholesale_app/wsgi.py"
    }
  ]
}
