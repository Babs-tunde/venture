{
  "version": 2,
  "builds": [
    {
      "src": "wholesale_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "wholesale_app/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PYTHONUNBUFFERED": "1"
    },
    "buildCommand": "pip install -r requirements.txt"
  }
}
